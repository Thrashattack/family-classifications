{
  "name": "family-classification",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Carlos Cunha",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start:server": "node dist/shared/infra/http/server.js",
    "lint": "eslint src --ext ts",
    "pretty": "prettier --write \"./src/**/*.{js,jsx,json,ts,tsx}\"",
    "tsc": "tsc --noEmit"
  },
  "engines": {
    "node": ">=12.3.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "prettier": "^2.2.1",
    "tsc": "^1.20150623.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
}
